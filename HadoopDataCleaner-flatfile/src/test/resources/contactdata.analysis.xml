<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<job xmlns="http://eobjects.org/analyzerbeans/job/1.0">
    <job-metadata>
        <job-description>Created with DataCleaner Community edition 3.7.1</job-description>
        <author>tomaszg</author>
        <updated-date>2014-11-16+01:00</updated-date>
    </job-metadata>
    <source>
        <data-context ref="Contact data"/>
        <columns>
            <column id="col_0" path="Company" type="STRING"/>
            <column id="col_1" path="Addressline1" type="STRING"/>
            <column id="col_2" path="Addressline2" type="STRING"/>
            <column id="col_3" path="Addressline3" type="STRING"/>
            <column id="col_4" path="Addressline4" type="STRING"/>
            <column id="col_5" path="Country" type="STRING"/>
            <column id="col_6" path="Phone number" type="STRING"/>
        </columns>
    </source>
    <transformation>
        <transformer>
            <descriptor ref="Concatenator"/>
            <properties>
                <property name="Separator" value="&amp;#44;"/>
            </properties>
            <input ref="col_1"/>
            <input ref="col_2"/>
            <input ref="col_3"/>
            <input ref="col_4"/>
            <output id="col_7" name="concat_address"/>
        </transformer>
    </transformation>
    <analysis>
        <analyzer>
            <descriptor ref="Create CSV file"/>
            <properties>
                <property name="File" value="C:/Program Files/DataCleaner/datastores/output-Contact data-Concatenator.csv"/>
                <property name="Separator char" value="&amp;#44;"/>
                <property name="Quote char" value="&amp;quot;"/>
                <property name="Escape char" value="\"/>
                <property name="Include header" value="true"/>
            </properties>
            <input ref="col_7"/>
            <input ref="col_1"/>
            <input ref="col_2"/>
            <input ref="col_3"/>
            <input ref="col_4"/>
        </analyzer>
    </analysis>
</job>
